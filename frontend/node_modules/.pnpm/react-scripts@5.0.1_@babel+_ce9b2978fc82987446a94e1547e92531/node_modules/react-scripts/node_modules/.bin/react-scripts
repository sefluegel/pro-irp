#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules/react-scripts/bin/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules/react-scripts/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules/react-scripts/bin/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules/react-scripts/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/react-scripts@5.0.1_@babel+_ce9b2978fc82987446a94e1547e92531/node_modules:/mnt/c/Users/flueg/Documents/GitHub/pro-irp/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
